{
    "name": "edulazaro/laractions",
    "description": "Actions for Laravel",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10"
    },
    "autoload": {
        "psr-4": {
            "EduLazaro\\Laractions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EduLazaro\\Laractions\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edu Lazaro",
            "email": "edu@edulazaro.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "EduLazaro\\Laractions\\LaractionsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.34"
    }
}
